-- INSERT, DELETE, UPDATE, SELECT
SELECT * 
FROM PROFESSOR
WHERE LENGTH(PAY) >= 6;

UPDATE PROFESSOR
SET 
    PAY = PAY + 50
WHERE LENGTH(ID) >= 6;

SELECT * FROM STUDENT;

INSERT INTO STUDENT(STU_NO, STU_NAME, STU_DEPT)
VALUES('12341234', '홍길동', '기계');

DELETE FROM STUDENT;

COMMIT;

ROLLBACK;
